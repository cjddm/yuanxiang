.v-f{
	width: 94%;
	margin: 5rpx 3% 0;
	box-shadow: 1px 1px 10px -2px rgb(201, 204, 204);
}
.v-one{
	font-size: 17px;
	margin-top: 10rpx;
	margin-left: 2px;
}
.v-zero{
	margin: 0 7rpx;
}
.v-two{
	margin-top: 22rpx;
	text-indent: 2em;
	font-size: 15px;
	line-height: 7vw;
	letter-spacing: 0.08em;
	text-align: justify;
}
.and{
	margin-top: 22px;
	margin-right: 3%;
	text-align: right;
}
.xinc-text{
	font-size: 14px;
	display: inline-block;
	margin-right: 2px;
}

.from{
	font-size: 13px;
	margin-bottom: 13px;
	margin-right: -2px;
}
.time{
	float: left;
	font-size: 27rpx;
	margin-top: 3px;
	color: rgb(201, 204, 204);
}
.xc-f{
	float: right;
	margin-right: 2%;
	margin-top: -10px;
}
.xin-collect{
	margin-right: -3px;
}
.xin-collect image{
	width: 40rpx;
	height: 40rpx;
	vertical-align: middle;
}
.plone{
  width: 94%;
  margin: 10px 3%;
	background-color: rgb(245, 245, 245);
	border-radius: 3px;
  padding-left: 3px;
}
.fxfather{
	height: 30px;
	width: 100%;
}
.fx{
	float: right;
	margin-right: 3%;
	overflow: hidden;
  color: rgb(117, 180, 216);
  font-weight: 700;
  background-color: rgb(250, 246, 246);
  padding: 2px 7px;
  border-radius: 5px;
}
.notpl{
	color: rgb(218, 218, 218);
	width: 64px;
  margin-left: auto;
  margin-right: auto;
}

.plimg{
	float: right;
	margin-right: 3%;
	width: 22px;
	height: 22px;
	overflow: hidden;
}
.pltext{
	font-size: 15px;
}
.show{
	height: 22px;
	width: 100%;
}
.pllist{
	width: 94%;
	margin-left: 3%;
	margin-right: 3%;
	font-size: 14px;
	margin-top: 20px;
	padding-bottom: 20px;
	/* box-shadow: 0px 3px 0px -2px rgb(238, 237, 237); */
}
.plname{
	font-size: 13px;
}
.block{
	/* padding-bottom: 20px; */
	box-shadow: 0px 3px 0px -2px rgb(250, 248, 248);
	padding: 0 3px 20px 3px;
}
.plvalue{
	margin-left: 1em;
	margin-bottom: 2px;
	text-align: justify;
}
.pltime{
	float: right;
	margin-right: 3px;
	font-size: 27rpx;
	color: rgb(201, 204, 204);
}
